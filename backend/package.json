{
  "name": "a2a-agent-scheduler",
  "version": "1.0.0",
  "description": "A2A智能体调度系统后端服务",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "init-db": "node scripts/init-database.js",
    "test": "jest",
    "start-python": "cd python_service && python start_service.py",
    "start-python-dev": "cd python_service && python start_service.py --reload",
    "install-python-deps": "cd python_service && pip install -r requirements.txt",
    "start-all": "concurrently \"npm run start-python\" \"npm start\"",
    "dev-all": "concurrently \"npm run start-python-dev\" \"npm run dev\""
  },
  "keywords": [
    "a2a",
    "agent",
    "scheduler",
    "ai"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.12",
    "sqlite3": "^5.1.6",
    "systeminformation": "^5.27.6",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
